    <!-- http://purl.obolibrary.org/obo/PATHGO_0000295 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000295">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000294"/>
        <oboInOwl:hasDbXref>GO:0085034</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">suppresses NF-kappaB signaling in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that modulates NF-kappaB signaling in another organism by suppressing NF-kappaB signaling so that the effector subunit is not able to initiate transcription in the nucleus.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000296 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000296">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000294"/>
        <oboInOwl:hasDbXref>GO:0085033</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">enhances NF-kappaB signaling in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that modulates NF-kappaB signaling in another organism by enhancing host NF-kappaB signaling by promoting activation.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000300 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000300">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000165"/>
        <rdfs:label xml:lang="en">disrupts STING signaling in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that disrupts cellular signaling in another organism by disrupting host stimulator of interferon genes (STING) signaling via targeting STING directly or affecting a protein proximal to STING that interferes with the proper recognition of pathogen activity.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000302 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000302">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000165"/>
        <rdfs:label xml:lang="en">disrupts JAK-STAT signaling in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that disrupts cellular signaling in another organism by disrupting host Janus kinase signal transducer and activator of transcription (JAK-STAT) signaling via targeting a sequence proximal to JAKs, STATs, protein inhibitors of activated STATs (PIAS), or suppressors of cytokine signaling (SOCS).</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000304 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000304">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000165"/>
        <rdfs:label xml:lang="en">disrupts PKR activity in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that disrupts cellular signaling in another organism by disrupting host protein kinase R (PKR) activity via pre-emptively binding pathogen dsRNA to sequester it from PKR, interfering with the phosphorylation of PKR by proximal effectors, or binding PKR directly.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000306 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000306">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000165"/>
        <rdfs:label xml:lang="en">disrupts RIG-1 signaling in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that disrupts cellular signaling in another organism by disrupting host cytosolic signaling proximal to the pattern recognition receptor retinoic acid-inducible gene I (RIG-1) via altering the ubiquitination of RIG-1, downregulating RIG-1 activity, reducing RIG-1 abundance, or impeding the interaction of RIG-1 with downstream effectors such as MAVS.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000308 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000308">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000085"/>
        <oboInOwl:hasDbXref>GO:0042786</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">disrupts antigen presentation in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that mediates immune evasion and subversion in another organism by disrupting host antigen presentation via inhibiting MHC I/II production or display, manipulating production or display of the antigen or epitope by altering activity of the proteasome or the TAP complex, or specifically targeting a particular parasite antigen for destruction.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000310 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000310">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000085"/>
        <rdfs:label xml:lang="en">suppresses cytokine activity in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that mediates immune evasion and subversion in another organism by suppressing cytokine activity, for example by sequestering or destroying the cytokine so it is not available to participate in signaling.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000312 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000312">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000085"/>
        <rdfs:label xml:lang="en">mediates concealment of foreign nucleic acid in another organism</rdfs:label>
        <skos:editorialNote>A mechanism that mediates immune evasion and subversion in another organism by mediating concealment of pathogen nucleic acid (typically carried out by dsRNA nucleic acid binding) for evasion of host pathogen recognition receptors.</skos:editorialNote>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000314 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000314">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000165"/>
        <rdfs:label xml:lang="en">modulates TRAF signaling in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that disrupts cellular signaling in another organism by modulating host TNF receptor-associated factor (TRAF) signaling.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000316 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000316">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000006"/>
        <obo:IAO_0000115>A mechanism that modulates protein synthesis in another organism by modulating eukaryotic translation elongation.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">modulates eukaryotic translation elongation in another organism</rdfs:label>
        <skos:editorialNote>https://orcid.org/0000-0002-5702-4690</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000321 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000321">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000236"/>
        <obo:IAO_0000115>A mechanism that disrupts the cell endomembrane system in another organism by modulating trafficking of vesicles to other cellular compartments that occur as part of normal host function.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">modulates endosomal vesicle trafficking in another organism</rdfs:label>
        <skos:editorialNote>Many parasite proteins, particularly from intracellular bacteria pathogens, inhibit or redirect host vesicle trafficking.

Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000322 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000322">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000150"/>
        <obo:IAO_0000115>A mechanism that promotes survival in host by mediating resistance to antibiotics.</obo:IAO_0000115>
        <oboInOwl:hasDbXref>ARO:1000001</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>GO:0046677</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">mediates resistance to antibiotics</rdfs:label>
        <skos:editorialNote>No subclasses.  Antibiotic resistance is well-covered by the Comprehensive Antibiotic Resistance Database (CARD) Ontology.</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000323 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000323">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000167"/>
        <obo:IAO_0000115>A mechanism that disrupts cellular transport in another organism by disrupting intracellular transport.</obo:IAO_0000115>
        <oboInOwl:hasDbXref>GO:0052038</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>GO:1990215</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">disrupts intracellular transport in another organism</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000324 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000324">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000341"/>
        <obo:IAO_0000115>A mechanism that suppresses formation of the terminal complement complex in another organism by recruitment of host vitronectin</obo:IAO_0000115>
        <rdfs:label xml:lang="en">suppresses complement system by recruiting vitronectin in another organism</rdfs:label>
        <skos:editorialNote>For instances where host vitronectin binding is used to facilitate adhesion and invasion, the term &apos;mediates host vitronectin binding&apos; in the &apos;enables adherence and colonization&apos; branch should be used. 

PMID: 20807208</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000325 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000325">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000164"/>
        <obo:IAO_0000115>A mechanism that disrupts cellular metabolism in another organism through modulation or redirection of E3 ubiquitin ligase, E3 ubiquitin ligase activity, E2 enzymes, or removal of ubiquitin from a target protein.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">modulates ubiquitin dynamics in another organism</rdfs:label>
        <skos:editorialNote>https://orcid.org/0000-0002-5702-4690</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000326 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000326">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000164"/>
        <obo:IAO_0000115>A mechanism that disrupts cellular metabolism by modulating transcription in host cells of another organism.</obo:IAO_0000115>
        <oboInOwl:hasDbXref>GO:0052026</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">modulates transcription in another organism</rdfs:label>
        <skos:editorialNote>https://orcid.org/0000-0002-5702-4690</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000327 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000327">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000164"/>
        <obo:IAO_0000115>A mechanism that disrupts cellular metabolism in another organism by mediating the cleavage of host DNA.</obo:IAO_0000115>
        <oboInOwl:hasRelatedSynonym>DNase activity</oboInOwl:hasRelatedSynonym>
        <rdfs:label xml:lang="en">mediates DNA cleavage in another organism</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000328 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000328">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000175"/>
        <rdfs:label xml:lang="en">modulates small GTPase activity in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that modulates G-protein signaling pathways in another organism by modulating host small GTPase activity.  This manipulation can occur through direct binding of host small GTPases or by alteration of the activity or abundance of the proximal effectors of small GTPases, including GTPase activating proteins (GAPs), guanine exchange factors (GEFs), or guanine dissociation inhibitors (GDIs).</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000329 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000329">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000085"/>
        <obo:IAO_0000115>A mechanism that mediates immune evasion and subversion in another organism by modulating the post-translational modification of ISGylation of host cell proteins.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">modulates ISGylation in another organism</rdfs:label>
        <skos:editorialNote>For example, interferon-stimulated gene 15 (ISG15), induced by type I inteferon, acts like a ubiquitin-like tag on host proteins.  The consequences of ISGylation are not well understood, but some studies have shown that ISGylated proteins have antiviral functions.

More functions of ISG15 reviewed in PMID: 30428561</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000330 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000330">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000329"/>
        <rdfs:label xml:lang="en">mediates deISGylation in another organism</rdfs:label>
        <skos:editorialNote>A mechanism that modulates ISGylation in another organism by removing or cleaving ISG15 tags from host proteins</skos:editorialNote>
        <skos:editorialNote>SARS-CoV2 and Crimean Congo Hemorrhagic fever both have proteases that act as deubiquitinases but also serve to remove ISG15 units from host proteins.

Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000331 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000331">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000333"/>
        <oboInOwl:hasDbXref>GO:0044071</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">modulates cell cycle in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that mediates pathogenicity by modulating the cell cycle of a cell in another organism.</obo:IAO_0000115>
        <skos:editorialNote>Elaborate subclasses</skos:editorialNote>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000332 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000332">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000333"/>
        <oboInOwl:hasDbXref>GO:0052040</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>GO:0052150</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">modulates apoptosis in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that mediates pathogenicity by modulating apoptosis in another organism. Gene products produced by pathogenic microbes may activate or prevent apoptosis to enable growth inside the host.</obo:IAO_0000115>
        <skos:editorialNote>Evaluate for placement in proliferation and survival</skos:editorialNote>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000333 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000333">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000114"/>
        <obo:IAO_0000115>A mechanism of pathogenicity that mediates a damaging effect directly.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">direct mechanism of pathogenicity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000334 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000334">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000332"/>
        <oboInOwl:hasDbXref>GO:0033668</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>GO:0052041</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">suppresses apoptosis in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that modulates apoptosis by suppressing apoptosis in host cells of another organism.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000335 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000335">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000332"/>
        <oboInOwl:hasDbXref>GO:0052042</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>GO:0052151</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">activates apoptosis in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that modulates apoptosis by activating apoptosis in host cells of another organism.</obo:IAO_0000115>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000336 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000336">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000114"/>
        <obo:IAO_0000115>A mechanism of pathogenicity that mediates a damaging effect indirectly.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">indirect mechanism of pathogenicity</rdfs:label>
        <skos:editorialNote>Does the gene product itself cause a pathogenic effect, or does it play a supporting role for pathogenesis to other gene products?  Note, functions related to the normal growth processes of pathogens like &apos;sporulation&apos; should not be annotated with this term.  Only genes that are necessary for pathogenesis, like &apos;secretion systems&apos;, should be annotated with this term.</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000337 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000337">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000336"/>
        <obo:IAO_0000115>A mechanism that mediates pathogenicity indirectly by mediating the synthesis of toxins.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">mediates toxin synthesis</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000338 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000338">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000336"/>
        <obo:IAO_0000115>A mechanism that mediates pathogenicity indirectly by potentiating the activity of effector molecules that act in another organism.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">potentiates activity of effector molecule that acts in another organism</rdfs:label>
        <skos:editorialNote>Examples include protecting a virulence factor from degradation, increasing the activity of a virulence factor, or assisting with the folding of a virulence factor (chaperone activity).  Note, term label was changed from &apos;mediates virulence factor activity&apos; so that it can be used for annotating snake venom components.

Examples genes:

Phospholipase A2 homolog crotoxin acid subunit CA from Crotalus durissus terrificus (South American rattlesnake): This rattlesnake venom component is not toxic in itself but potentiates the lethal potency of the other two constituents of a heterotrimer: CA and CB which are lethal presynaptic neurotoxins that block neuromuscular transmission [PMID21787789].

Botulinum neurotoxin type C1 nontoxic-nonhemagglutinin component (CST085; NTNHA) from Clostridium botulinum phage: This protein is nearly identical to NTNHA which complexes with the botulinum toxin to protect it from destruction in the gastrointestinal tract, serving as a chaperone [PMID22363010].

Acidic phospholipase A2 RV-7 from Daboia siamensis (Eastern Russel&apos;s viper) (P31100): As a monomer, this snake venom component has low enzymatic activity. It may facilitate the binding of venom component RV-4 to presynaptic binding sites, acting as a chaperone and minimizing the destruction of RV-4 by binding to extraneous targets. The RV-7/RV-4 heterodimer targets presynaptic sites at the neuromuscular junction [PMID8835338].

Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000340 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000340">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000028"/>
        <obo:IAO_0000115>A mechanism that modulates the cytoskeleton in cells of another organism by mediating host actin stabilization.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">mediates actin stabilization in another organism</rdfs:label>
        <skos:editorialNote>Cells infected with enterohemorrhagic Escherichia coli (EHEC) lacking the T3SS effectors EspO1-1 and EspO1-2 display cell rounding phenotypes and lose focal adhesions.  These effectors work with host proteins (like GTPases) to stabilize actin and focal adhesions during infection [PMID 23409096].

Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000341 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000341">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000100"/>
        <rdfs:label xml:lang="en">mediates binding of complement regulatory protein in another organism</rdfs:label>
        <obo:IAO_0000115>A mechanism that mediates resistance to the complement system in another organism by mediating binding of a complement regulatory protein, such as a host enzyme, to the parasite surface.</obo:IAO_0000115>
        <skos:editorialNote>PMID: 26808444</skos:editorialNote>
        <skos:editorialNote>Term suggested by Gene Goldbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000343 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000343">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATHGO_0000236"/>
        <obo:IAO_0000115>A mechanism that modulates host cell endomembrane dynamics by contributing to the formation and/or maintenance of a vacuole in another organism.</obo:IAO_0000115>
        <oboInOwl:hasDbXref>GO:0044075</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>GO:0085018</oboInOwl:hasDbXref>
        <rdfs:label xml:lang="en">mediates pathogen vacuole formation/maintenance in another organism</rdfs:label>
        <skos:editorialNote>Term suggested by Gene Godbold (https://orcid.org/0000-0002-5702-4690)</skos:editorialNote>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000345 -->

