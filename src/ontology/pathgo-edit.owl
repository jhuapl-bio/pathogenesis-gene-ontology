        <rdfs:label>mediates iron acquisition from host</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PATHGO_0000069 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PATHGO_0000069">
